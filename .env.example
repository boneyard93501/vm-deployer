# Fluence VM Deployer Environment Variables
# Copy this to .env and fill in your actual values

# Required: Your Fluence API key
FLUENCE_API_KEY=your_fluence_api_key_here

# Required: SSH public keys (comma-separated for multiple keys)
SSH_KEYS=ssh-rsa AAAAB3NzaC1yc2EAAA... user@host,ssh-ed25519 AAAAC3NzaC1lZDI1NTE5... user@host2

# Optional: Log file path (defaults to ./logs/vm_deployment.log)
LOG_FILE_PATH=./logs/vm_deployment.log